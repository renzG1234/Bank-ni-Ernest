/**
 *
 * @author User
 */
public class ErnestStudent {
    private String name;
    private int id;

    public ErnestStudent(String name, int id) {
        this.name = name;
        this.id = id;
    }

    public void displayInfo() {
        System.out.println("Student ID: " + id + ", Name: " + name);
    }
}
